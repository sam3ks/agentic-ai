{
  "test_name": "Account Creation Flow",
  "steps": [
    { "input": "open an account", "expect": "email address" },
    { "input": "sam@samm.com", "expect": "pan number" },
    { "input": "CESUI1234L", "expect": "aadhaar number" },
    { "input": "123456789012", "expect": "aadhaar name" },
    { "input": "Sam S", "expect": "aadhaar father name" },
    { "input": "Adam A", "expect": "aadhaar dob" },
    { "input": "12-10-1999", "expect": "aadhaar address" },
    { "input": "Bangalore", "expect": "aadhaar mobile" },
    { "input": "2123123412", "expect": "account type" },
    { "input": "savings", "expect": "account created successfully" }
  ]
}

